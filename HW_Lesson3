    }; 

    Object.size = function(obj) {
        var size = 0,
          key;
        for (key in obj) {
          if (obj.hasOwnProperty(key)) size++;
        }
        return size;
      };

      var size = Object.size(post);
      console.log(size);

      console.log(Object.values(post));


